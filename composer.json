{
	"name": "serato/jwt",
	"homepage": "https://github.com/serato/web-sws-php-jwt",
	"description": "JWT handling for Serato Web Services",
	"type": "library",
	"require": {
		"php": "^7.1",
		"spomky-labs/jose": "~7.0.0",
		"aws/aws-sdk-php": "~3.0",
		"ramsey/uuid": "~3.6"
	},
	"require-dev": {
		"phpunit/phpunit": "~7.0",
		"squizlabs/php_codesniffer": "~2.0",
		"symfony/cache": "^4.3.8",
		"phpstan/phpstan": "^0.11.0",
		"phpstan/phpstan-phpunit": "^0.11.0",
        "apigen/apigen": "dev-master#85290e8",
		"roave/better-reflection": "3.3.0 as dev-master",
		"phpstan/phpstan-mockery": "^0.11.0",
		"mockery/mockery": "^1",
		"enlightn/security-checker": "^1.4"
	},
	"autoload": {
		"psr-4": {
			"Serato\\Jwt\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Serato\\Jwt\\Test\\": "tests/"
		}
	}
}
