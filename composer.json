{
	"name": "serato/jwt",
	"homepage": "https://github.com/serato/web-sws-php-jwt",
	"description": "JWT handling for Serato Web Services",
	"type": "library",
	"require": {
		"php": "^8.2",
		"web-token/jwt-framework": "~3.2",
		"aws/aws-sdk-php": "~3.0",
		"ramsey/uuid": "~3.6"
	},
	"require-dev": {
		"phpunit/phpunit": "~10.0",
		"squizlabs/php_codesniffer": "~2.0",
		"symfony/cache": "^6.3",
		"phpstan/phpstan": "^1.10",
		"mockery/mockery": "^1",
		"enlightn/security-checker": "^1.4"
	},
	"autoload": {
		"psr-4": {
			"Serato\\Jwt\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Serato\\Jwt\\Test\\": "tests/"
		}
	}
}
