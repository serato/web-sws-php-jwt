{
	"name": "serato/jwt",
	"homepage": "https://github.com/serato/web-sws-php-jwt",
	"description": "JWT handling for Serato Web Services",
	"type": "library",
	"require": {
		"php" : "^7.0",
		"spomky-labs/jose": "~6.1.0",
		"aws/aws-sdk-php": "~3.0",
		"ramsey/uuid": "~3.6"
	},
	"require-dev": {
		"phpunit/phpunit": "~6.0",
		"squizlabs/php_codesniffer": "~2.0",
		"symfony/cache": "v3.3.0-RC1"
	},
	"autoload": {
		"psr-4": {
			"Serato\\Jwt\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Serato\\Jwt\\Test\\": "tests/"
		}
	}
}
